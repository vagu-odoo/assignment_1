<?xml version="1.0"?>
<odoo>
    <record id="stock_batch_picking_transfer_form" model="ir.ui.view">
        <field name="name">stock.batch.picking.transfer.inherited.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='user_id']" position='after'>
                <field name="volume_picking"/>
                <field name="weight_picking"/>
            </xpath>
        </field>

    </record>

    
</odoo>